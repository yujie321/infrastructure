            <corners android:radius="8dp" />
            <gradient android:angle="0" android:centerX="0.3" android:centerY="0.7"
                android:endColor="#FFFFFD" android:startColor="#FFFFFD" android:type="linear"
                android:useLevel="false" />
        </shape>
    </item>
