<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#B5B5B5">
    <item>
        <shape android:shape="rectangle">
            <!--<solid android:color="@color/fun_car_colorPrimary" />-->
            <corners android:radius="8dp" />
            <!--<stroke-->
                <!--android:width="1dp"-->
                <!--android:color="@color/fun_car_colorPrimary" />-->
            <gradient android:angle="0" android:centerX="0.3" android:centerY="0.7"
                android:endColor="#FFFFFD" android:startColor="#FFFFFD" android:type="linear"
                android:useLevel="false" />
        </shape>
    </item>
    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
